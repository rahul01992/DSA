bool searchInBST(BinaryTreeNode<int> *root , int k) {
	if(root==NULL){
		return false;
	}
	if(root->data==k){
		return true;
	}
	
	bool ans=false;
	if(root->right!=NULL && root->data<k){
	     ans=searchInBST(root->right,  k);
		 
	}
    if (root->left!=NULL && root->data > k) {
          ans=searchInBST(root->left, k);  
        }
		return ans;
}
